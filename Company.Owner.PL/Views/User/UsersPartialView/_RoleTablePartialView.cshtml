@model IEnumerable<UserToReturnDto>;
@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td>@item.Email</td>
        <td>
            @foreach (var role in item.Roles)
            {
                <span>@role</span>
            }
        </td>
        <partial name="_buttonsPartialView" model="@item.Id" />
    </tr>
}
        